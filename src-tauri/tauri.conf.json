{
  "build": {
    "beforeDevCommand": "trunk serve",
    "beforeBuildCommand": "trunk build",
    "devPath": "http://0.0.0.0:8080",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "intauri",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "CardGame",
      "copyright": "Copyright Â© 2022 mhfan",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.mhfan",
      "shortDescription": "24-Game/Puzzle/Challenge",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
	"url": "/inyew/",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "title": "24 Puzzle"
      }
    ]
  }
}
